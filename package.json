{
  "name": "@bcommonslab/common-knowledge-scout",
  "version": "1.0.5",
  "description": "Common Knowledge Scout - Modern Document Management System",
  "main": "dist/server.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "public/**/*",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "document-management",
    "next.js",
    "typescript",
    "file-management",
    "knowledge-scout"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/bCommonsLAB/CommonKnowledgeScout.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "private": false,
  "license": "AGPL-3.0-only",
  "packageManager": "pnpm@9.15.3",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:web": "next build",
    "docs:build": "mkdocs build",
    "docs:serve": "mkdocs serve",
    "build:package": "cross-env IS_PACKAGE_BUILD=true BUILD_TARGET=package pnpm exec next build && node scripts/prepare-package.js",
    "package:prepare": "node scripts/prepare-package.js",
    "package:publish": "pnpm run build:package && pnpm publish",
    "start": "next start",
    "lint": "next lint",
    "test": "pnpm run test:unit",
    "test:watch": "pnpm run test:unit:watch",
    "test:node": "node --import tsx --test \"tests/**/*.test.ts\" --test-reporter=spec",
    "test:node:watch": "node --import tsx --test \"tests/**/*.test.ts\" --watch --test-reporter=spec",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:package": "node test-package.js",
    "export:cursor-chats": "node --import tsx scripts/export-cursor-chats.ts",
    "docs:history:merge": "node --import tsx scripts/merge-history.ts",
    "docs:history": "pnpm run docs:history:merge",
    "docs:history:monthly": "node --import tsx scripts/merge-history-monthly.ts",
    "docs:history:mon": "pnpm run docs:history:monthly",
    "test:vector-search": "node --import tsx scripts/test-mongodb-vector-search.ts"
  },
